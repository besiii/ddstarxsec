ifeq ($(INSTALLAREA),)
installarea = $(CMTINSTALLAREA)
else
ifeq ($(findstring `,$(INSTALLAREA)),`)
installarea = $(shell $(subst `,, $(INSTALLAREA)))
else
installarea = $(INSTALLAREA)
endif
endif

install_dir = ${installarea}/${CMTCONFIG}/lib
Kpiinstallname = $(library_prefix)Kpi$(library_suffix).$(shlibsuffix)

Kpi :: Kpiinstall

install :: Kpiinstall

Kpiinstall :: $(install_dir)/$(Kpiinstallname)
	@if test ! "${installarea}" = ""; then\
	  echo "installation done"; \
	fi

$(install_dir)/$(Kpiinstallname) :: $(bin)$(Kpiinstallname)
	@if test ! "${installarea}" = ""; then \
	  cd $(bin); \
	  if test ! "$(install_dir)" = ""; then \
	    if test ! -d "$(install_dir)"; then \
	      mkdir -p $(install_dir); \
	    fi ; \
	    if test -d "$(install_dir)"; then \
	      echo "Installing library $(Kpiinstallname) into $(install_dir)"; \
	      if test -e $(install_dir)/$(Kpiinstallname); then \
	        $(cmt_uninstall_area_command) $(install_dir)/$(Kpiinstallname); \
	        $(cmt_uninstall_area_command) $(install_dir)/$(Kpiinstallname).cmtref; \
	      fi; \
	      $(cmt_install_area_command) `pwd`/$(Kpiinstallname) $(install_dir)/$(Kpiinstallname); \
	      echo `pwd`/$(Kpiinstallname) >$(install_dir)/$(Kpiinstallname).cmtref; \
	    fi \
          else \
	    echo "Cannot install library $(Kpiinstallname), no installation directory specified"; \
	  fi; \
	fi

Kpiclean :: Kpiuninstall

uninstall :: Kpiuninstall

Kpiuninstall ::
	@if test ! "${installarea}" = ""; then \
	  cd $(bin); \
	  if test ! "$(install_dir)" = ""; then \
	    if test -d "$(install_dir)"; then \
	      echo "Removing installed library $(Kpiinstallname) from $(install_dir)"; \
	      $(cmt_uninstall_area_command) $(install_dir)/$(Kpiinstallname); \
	      $(cmt_uninstall_area_command) $(install_dir)/$(Kpiinstallname).cmtref; \
	    fi \
          else \
	    echo "Cannot uninstall library $(Kpiinstallname), no installation directory specified"; \
	  fi; \
	fi




